 pipeline {
  agent any
   stages {
     stage('stage1 testing variables') {
       steps {
         script {
             // declaring the variable
             def abc = "hello"
             def xyz = "world"
             // 1st method of calling the variable
             print abc
             print xyz

             // 2nd method od calling the variable

             print "abc = ${abc}"
             print "xyz = ${xyz}"



          }
       }
    }

  }

}

